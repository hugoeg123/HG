# Configurações do Servidor
PORT=5001
NODE_ENV=development

# Configurações do PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_NAME=health_guardian
DB_USER=postgres
DB_PASSWORD=postgres
DB_DIALECT=postgres

# (Opcional) URL completa para compatibilidade
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/health_guardian

# Configurações de JWT (necessário para login)
JWT_SECRET=dev_secret_for_local
JWT_EXPIRES_IN=24h

# Configurações de CORS
FRONTEND_URL=http://localhost:3000
ADMIN_URL=http://localhost:3001

# Flags de debug
DEBUG_HTTP=false
DEBUG_CORS=false