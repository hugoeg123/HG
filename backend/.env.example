# Configurações do Servidor
PORT=5000
NODE_ENV=development

# Configurações do PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_NAME=health_guardian
DB_USER=postgres
DB_PASSWORD=postgres
DB_DIALECT=postgres

# Configurações de JWT
JWT_SECRET=health_guardian_secret_key_change_in_production
JWT_EXPIRES_IN=24h

# Configurações de Criptografia
ENCRYPTION_KEY=32_char_encryption_key_for_sensitive_data

# Configurações de CORS
FRONTEND_URL=http://localhost:3000
ADMIN_URL=http://localhost:3001

# Configurações de API de IA (mock)
AI_API_ENABLED=true
AI_API_ENDPOINT=http://localhost:5000/api/ai/mock

# Configurações de Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
AUTH_RATE_LIMIT_MAX=5