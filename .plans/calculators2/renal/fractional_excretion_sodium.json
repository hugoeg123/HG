{
  "name": "Fração de excreção de sódio (FeNa)",
  "domain": "função renal",
  "description": "Calcula a porcentagem de sódio filtrado pelos rins que é excretado na urina. Utiliza concentrações de sódio e creatinina na urina e no plasma.",
  "inputs": [
    {"name": "Na_urina", "symbol": "sodium", "unit": "mEq/L", "description": "Concentração de sódio na urina"},
    {"name": "Na_plasma", "symbol": "sodium", "unit": "mEq/L", "description": "Concentração de sódio no plasma"},
    {"name": "Cr_urina", "symbol": "creatinine", "unit": "mg/dL", "description": "Concentração de creatinina na urina"},
    {"name": "Cr_plasma", "symbol": "creatinine", "unit": "mg/dL", "description": "Concentração de creatinina no plasma"}
  ],
  "output": {"name": "FeNa", "unit": "%", "description": "Fração de excreção de sódio"},
  "formula": "FeNa (%) = (Na_urina × Cr_plasma) / (Na_plasma × Cr_urina) × 100",
  "references": [
    {"citation": "World Journal of Hepatology – Fractional excretion of sodium formula", "url": "https://pubmed.ncbi.nlm.nih.gov/21634077/"}
  ]
}